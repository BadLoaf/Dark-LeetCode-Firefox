{

  "manifest_version": 2,
  "name": "DarkLeetcode",
  "version": "1.0",

  "description": "Gives dark theme to the leetcode project pages",

  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["darkify.js"]
    }
  ]

}

